<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 200" width="1200" height="200">
  <!-- Oil Rig 1 -->
  <g fill="#000000">
    <!-- Base Structure -->
    <rect x="50" y="150" width="100" height="50"/>
    <!-- Tower Structure -->
    <rect x="80" y="50" width="40" height="100"/>
    <!-- Top Structure -->
    <polygon points="60,50 140,50 120,20 80,20"/>
    <!-- Derrick Beams -->
    <line x1="80" y1="50" x2="60" y2="150" stroke="#000000" stroke-width="5"/>
    <line x1="120" y1="50" x2="140" y2="150" stroke="#000000" stroke-width="5"/>
    <!-- Horizontal Supports -->
    <rect x="70" y="80" width="60" height="5"/>
    <rect x="70" y="110" width="60" height="5"/>
  </g>

  <!-- Oil Rig 2 -->
  <g fill="#000000" transform="translate(250, 0)">
    <!-- Base Structure -->
    <rect x="50" y="160" width="120" height="40"/>
    <!-- Tower Structure -->
    <rect x="90" y="40" width="40" height="120"/>
    <!-- Top Structure -->
    <polygon points="70,40 150,40 130,10 90,10"/>
    <!-- Derrick Beams -->
    <line x1="90" y1="40" x2="50" y2="160" stroke="#000000" stroke-width="5"/>
    <line x1="130" y1="40" x2="170" y2="160" stroke="#000000" stroke-width="5"/>
    <!-- Horizontal Supports -->
    <rect x="80" y="70" width="60" height="5"/>
    <rect x="80" y="100" width="60" height="5"/>
    <rect x="80" y="130" width="60" height="5"/>
  </g>

  <!-- Oil Rig 3 -->
  <g fill="#000000" transform="translate(500, 0)">
    <!-- Base Structure -->
    <rect x="40" y="155" width="110" height="45"/>
    <!-- Tower Structure -->
    <rect x="75" y="45" width="40" height="110"/>
    <!-- Top Structure -->
    <polygon points="55,45 135,45 115,15 75,15"/>
    <!-- Derrick Beams -->
    <line x1="75" y1="45" x2="40" y2="155" stroke="#000000" stroke-width="5"/>
    <line x1="115" y1="45" x2="150" y2="155" stroke="#000000" stroke-width="5"/>
    <!-- Horizontal Supports -->
    <rect x="65" y="75" width="60" height="5"/>
    <rect x="65" y="105" width="60" height="5"/>
    <rect x="65" y="135" width="60" height="5"/>
  </g>

  <!-- Oil Rig 4 -->
  <g fill="#000000" transform="translate(750, 0)">
    <!-- Base Structure -->
    <rect x="45" y="150" width="100" height="50"/>
    <!-- Tower Structure -->
    <rect x="75" y="50" width="40" height="100"/>
    <!-- Top Structure -->
    <polygon points="55,50 135,50 115,20 75,20"/>
    <!-- Derrick Beams -->
    <line x1="75" y1="50" x2="45" y2="150" stroke="#000000" stroke-width="5"/>
    <line x1="115" y1="50" x2="145" y2="150" stroke="#000000" stroke-width="5"/>
    <!-- Horizontal Supports -->
    <rect x="65" y="80" width="60" height="5"/>
    <rect x="65" y="110" width="60" height="5"/>
  </g>

  <!-- Oil Rig 5 -->
  <g fill="#000000" transform="translate(950, 0)">
    <!-- Base Structure -->
    <rect x="50" y="160" width="120" height="40"/>
    <!-- Tower Structure -->
    <rect x="90" y="40" width="40" height="120"/>
    <!-- Top Structure -->
    <polygon points="70,40 150,40 130,10 90,10"/>
    <!-- Derrick Beams -->
    <line x1="90" y1="40" x2="50" y2="160" stroke="#000000" stroke-width="5"/>
    <line x1="130" y1="40" x2="170" y2="160" stroke="#000000" stroke-width="5"/>
    <!-- Horizontal Supports -->
    <rect x="80" y="70" width="60" height="5"/>
    <rect x="80" y="100" width="60" height="5"/>
    <rect x="80" y="130" width="60" height="5"/>
  </g>
</svg>